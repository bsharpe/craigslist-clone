cities = ["Seattle","New York","Los Angeles","Chicago","Portland", "Oakland", "San Francisco", "Memphis", "Atlanta"]

cities.each do |city|
  puts "Adding #{city}"
  c = City.find_or_create_by_name(city)
  c.save
end

categories = ["Community","Services","Housing","For Sale","Jobs"]

categories.each do |cat|
  puts "Adding #{cat}"
  c = Category.find_or_create_by_name(cat)
  c.permalink = cat.downcase
  c.save
end